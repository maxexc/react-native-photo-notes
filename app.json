{
  "expo": {
    "name": "Photo notes",
    "slug": "react-native-photo-notes",
    "description": "This is an excellent app for photo notes, based real-time data.",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [[
        "expo-camera",
        {"cameraPermission": "granted"
        }
    ]],
    "splash": {
      "image": "./assets/react-native-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
