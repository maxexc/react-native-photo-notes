{
  "expo": {
    "name": "Photo notes",
    "slug": "react-native-photo-notes",
    "description": "This is an excellent app for photo notes, based real-time data.",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "sdkVersion": "48.0.0",
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/e932c4c6-71a2-4446-bf38-cf4ef943479b"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "granted"
        }
      ]
    ],
    "splash": {
      "image": "./assets/react-native-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e932c4c6-71a2-4446-bf38-cf4ef943479b"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "owner": "max.exc7"
  }
}
